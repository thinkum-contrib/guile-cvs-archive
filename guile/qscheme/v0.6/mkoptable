#!/usr/bin/perl -n
#
if (/^VMPRIM\((\w+),,\s*\"(\S+)\"\s*,\s*(\S+)\)/) {
	print "\t{&&l_$1, \"$2\", $3,0,0,0 },\n";
}
if (/^Prim\((\w+),\s*\"(\S+)\"\s*,\s*(\S+)\)/) {
	print "\t{&&l_$1, \"$2\", $3,0,0,0 },\n";
}
if (/^PrimVarargs\((\w+),\s*\"(\S+)\"\)/) {
	print "\t{&&l_$1, \"$2\", -1,0,0,1 },\n";
}
if (/^PRIM\((\w+),\s*\"(\S+)\"\s*,\s*(\S+)\)/) {
	print "\t{&&l_$1, &&e_$1, \"$2\", $3},\n";
}
if (/^PRIMV\((\w+),\s*\"(\S+)\"\)/) {
	print "\t{&&l_$1, &&e_$1, \"$2\", -1,0,0,1 },\n";
}
